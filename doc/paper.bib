@inproceedings{streaming2010johansen,
	author = {Riiser, Haakon and Halvorsen, P\r{a}l and Griwodz, Carsten and Johansen, Dag},
	title = {Low Overhead Container Format for Adaptive Streaming},
	year = {2010},
	isbn = {9781605589145},
	publisher = {Association for Computing Machinery},
	address = {New York, NY, USA},
	doi = {10.1145/1730836.1730859},
	abstract = {Current segmented HTTP streaming systems provide scalable and quality adaptive video delivery services to a huge number of users. However, while they support a wide range of bandwidths and enable arbitrary content-based composition, their current formats have shortcomings like large overheads, live streaming delays, etc. We have therefore developed an adaptive media player that works around these problems while still using standard components like H.264/AVC for video, and MP3 for audio. The system's adaptivity allows the player to pick a quality level that makes good use of available bandwidth and CPU resources while at the same time maintaining smooth uninterrupted playback, as well as offering near instant seek and startup times.This paper presents an appropriate way of coding the segments and a simple multimedia container format that is optimized for adaptive streaming and video composition over HTTP. We show that our format is sufficiently advanced to contain any payload type, while being trivial to parse and translate to other container formats. Additionally, we show that our format is second to none in terms of overhead, without incurring any penalties on live streaming.},
	booktitle = {Proceedings of the First Annual ACM SIGMM Conference on Multimedia Systems},
	pages = {193–198},
	numpages = {6},
	keywords = {container format, multiplexing, adaptive streaming, low overhead, low latency, http, file format, video},
	location = {Phoenix, Arizona, USA},
	series = {MMSys '10}
}

